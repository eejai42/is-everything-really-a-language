@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix erb: <http://example.org/erb#> .

# === Shape with rules for Customers ===
erb:CustomersShape a sh:NodeShape ;
    sh:targetClass erb:Customers ;
    sh:rule [
        a sh:SPARQLRule ;
        rdfs:label "rule_Customers_FullName" ;
        sh:prefixes erb: ;
        sh:construct """
            PREFIX erb: <http://example.org/erb#>
            PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
            CONSTRUCT {
                $this erb:fullName ?_result .
            }
            WHERE {
    $this a erb:Customers .
    OPTIONAL { $this erb:firstName ?first_name . }
    OPTIONAL { $this erb:lastName ?last_name . }
                BIND(CONCAT(?first_name, " ", ?last_name) AS ?_result)
            }
        """ ;
    ] .

# === Shape with rules for Orders ===
erb:OrdersShape a sh:NodeShape ;
    sh:targetClass erb:Orders ;
    sh:rule [
        a sh:SPARQLRule ;
        rdfs:label "rule_Orders_Name" ;
        sh:prefixes erb: ;
        sh:construct """
            PREFIX erb: <http://example.org/erb#>
            PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>
            CONSTRUCT {
                $this erb:name ?_result .
            }
            WHERE {
    $this a erb:Orders .
    OPTIONAL { $this erb:orderNumber ?order_number . }
                BIND(CONCAT("ORD", ?order_number) AS ?_result)
            }
        """ ;
    ] ;
