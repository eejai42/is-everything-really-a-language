@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix erb: <http://example.org/erb#> .

erb: a owl:Ontology ;
    rdfs:label "ERB Ontology" ;
    rdfs:comment "Generated from effortless-rulebook.json" .

# === Class: Customers ===
erb:Customers a owl:Class ;
    rdfs:label "Customers" .

erb:customerId a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "CustomerId" .

erb:customer a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "Customer" .

erb:emailAddress a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "EmailAddress" .

erb:firstName a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "FirstName" .

erb:lastName a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "LastName" .

erb:fullName a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "FullName" .
    # calculated: calculated

erb:orders a owl:DatatypeProperty ;
    rdfs:domain erb:Customers ;
    rdfs:range xsd:string ;
    rdfs:label "Orders" .

# === Class: Orders ===
erb:Orders a owl:Class ;
    rdfs:label "Orders" .

erb:orderId a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:string ;
    rdfs:label "OrderId" .

erb:name a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:string ;
    rdfs:label "Name" .
    # calculated: calculated

erb:orderNumber a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:integer ;
    rdfs:label "OrderNumber" .

erb:customer a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:string ;
    rdfs:label "Customer" .

erb:customerEmailAddress a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:string ;
    rdfs:label "CustomerEmailAddress" .
    # calculated: lookup

erb:customerFullName a owl:DatatypeProperty ;
    rdfs:domain erb:Orders ;
    rdfs:range xsd:string ;
    rdfs:label "CustomerFullName" .
    # calculated: lookup
